			<xs:restriction base="StructuredRemittanceInformation7">
				<xs:sequence>
					<xs:element name="CdtrRefInf" type="AT_CreditorReferenceInformation2"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
